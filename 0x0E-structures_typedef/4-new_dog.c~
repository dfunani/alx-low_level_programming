#include "main.h"

/**
 * new_dog - creates a dog
 * @name: name of dog
 * @age: of dog
 * @owner: of dog
 *
 * Return: ptr to the dog created
 *
 */

dog_t *new_dog(char *name, float age, char *owner)
{
	dog_t dog_obj;
	dog_t *dog_ptr = &dog_obj;

	(*dog_ptr).name = name;
	(*dog_ptr).age = age;
	(*dog_ptr).owner = owner;
	return (dog_ptr);
}
