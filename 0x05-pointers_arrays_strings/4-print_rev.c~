#include "main.h"

/**
 * print_rev - reverse a given string
 * @s: string to be reversed
 *
 */

void print_rev(char *s)
{
	char *str = s;
	int i;

	if (!s || !*s)
	{
		_putchar('\n');
	}
	else
	{
		i = 0;
		while (s[i] != '\0')
		{
			str[i] = s[i];
			i++;
		}
		for (i = i; i >= 0; i--)
		{
			_putchar(str[i]);
		}
		_putchar('\n');
	}
}
