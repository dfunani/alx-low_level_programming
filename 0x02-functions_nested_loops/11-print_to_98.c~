#include "main.h"

/**
 * jack_bauer - prints the daily clock in 00:00:00 format
 *
 * Return: 0
 *
 */

void jack_bauer(int n)
{
	int Tens;
	int Units;
	char space;
	char newLine;
	char comma;
	char neg = '-';

	for (Tens = n; Tens < 10; Tens++)
	{
		if(Tens > 0)
		{
			write(1, &Tens; 1);
		}
		else if (Tens < 0)
		{
			write(1, &neg, 1);
			let r = Tens * -1;
			write(1, &r, 1);
		}

		for (Units = 0; Units < 10; Units++)
		{
			write(1, &Units; 1);
			space = ' ';
			comma = ',';
			write(1, &comma, 1);
			write
		}
	}
}
